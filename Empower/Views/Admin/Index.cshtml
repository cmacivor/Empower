@model AdminViewModel

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>@Model.AdminType</h1>
<div id="root"></div>

@if (Model.AdminType == "serviceprogramcategory")
{
    @section scripts {
        <script src="~/dist/serviceprogramcategoryapp.bundle.js"></script>
    }
}
else
{
    @section scripts {
        <script src="~/dist/admintypeapp.bundle.js"></script>
    }
}





